{
  // "editor.fontFamily": "MesloLGM Nerd Font,Consolas,Menlo,monospace",
  "editor.fontFamily": "FiraCode NF",
  "editor.fontLigatures": true,
  // "editor.fontSize": 14,
  "editor.fontWeight": "500",
  "editor.formatOnPaste": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.disableExtension": false,
  "vim.highlightedyank.enable": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "<Enter>"],
      "commands": [":nohl"]
    },
    {
      "before": ["leader", "w"],
      "commands": [
        {
          "command": ":wa"
        }
      ]
    },
    {
      "before": ["K"],
      "after": ["i", "<cr>", "<esc>"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["0"],
      "after": ["g", "0"]
    },
    {
      "before": ["$"],
      "after": ["g", "$"]
    },
    {
      "before": ["<Space>"],
      "commands": ["editor.toggleFold"]
    }
  ],
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-t>": false,
    "<C-q>": true,
    "<C-a>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false
  },
  // "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontFamily": "MesloLGM Nerd Font,Consolas,Menlo,monospace",
  "terminal.integrated.fontSize": 14,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "terminal.external.windowsExec": "",
  "files.associations": {
    "*jenkinsfile": "groovy",
    "*.snap": "markdown"
  },
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "explorer.confirmDragAndDrop": false,
  "hediet.vscode-drawio.theme": "min",
  "gitlens.views.commits.files.layout": "tree",
  "editor.formatOnType": true,
  "files.exclude": {
    "**/.git": false
  },
  "debug.onTaskErrors": "showErrors",
  "git.closeDiffOnOperation": true,
  "workbench.startupEditor": "none",
  "redhat.telemetry.enabled": false,
  "azureTerraform.terminal": "integrated",
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.windows": "C:\\Users\\trey.mack\\.vs-kubernetes\\tools\\helm\\windows-amd64\\helm.exe",
