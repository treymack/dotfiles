{
  // "editor.fontFamily": "MesloLGM Nerd Font,Consolas,Menlo,monospace",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  // "editor.fontSize": 14,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.disableExtension": false,
  "vim.highlightedyank.enable": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "<Enter>"],
      "commands": [":nohl"]
    },
    {
      "before": ["leader", "w"],
      "commands": [
        {
          "command": ":wa"
        }
      ]
    },
    {
      "before": ["K"],
      "after": ["i", "<cr>", "<esc>"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["0"],
      "after": ["g", "0"]
    },
    {
      "before": ["$"],
      "after": ["g", "$"]
    }
  ],
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-t>": false,
    "<C-q>": true,
    "<C-a>": false,
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false
  },
  // "terminal.integrated.fontFamily": "Fira Code",
